{
    "name": "Aeon Digital Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#111111",
        "editor.lineHighlightBackground": "#222222",
        "editor.selectionBackground": "#333333",
        "editor.foreground": "#CCCCCC",
        "editorCursor.foreground": "#AAAAAA",
        "editorWhitespace.foreground": "#888888",
        "editorIndentGuide.background": "#333333",
        "editorIndentGuide.activeBackground": "#333333"
    },
    "tokenColors": [
        {
            "name": "Makefile :: Comment Line Hash",
            "scope": [
                "punctuation.definition.comment.makefile"
            ],
            "settings": {
                "foreground": "#666666"
            }
        },
        {
            "name": "Makefile :: Comment Line Content",
            "scope": [
                "comment.line.number-sign.makefile"
            ],
            "settings": {
                "foreground": "#666666"
            }
        },
        {
            "name": "Makefile :: Support Built-in Function ",
            "scope": [
                "support.function.target.PHONY.makefile",
                "support.function.target.SUFFIXES.makefile",
                "support.function.target.DEFAULT.makefile",
                "support.function.target.PRECIOUS.makefile",
                "support.function.target.INTERMEDIATE.makefile",
                "support.function.target.SECONDARY.makefile",
                "support.function.target.SECONDEXPANSION.makefile",
                "support.function.target.DELETE_ON_ERROR.makefile",
                "support.function.target.IGNORE.makefile",
                "support.function.target.LOW_RESOLUTION_TIME.makefile",
                "support.function.target.SILENT.makefile",
                "support.function.target.EXPORT_ALL_VARIABLES.makefile",
                "support.function.target.NOTPARALLEL.makefile",
                "support.function.target.ONESHELL.makefile",
                "support.function.target.POSIX.makefile"
            ],
            "settings": {
                "foreground": "#987C57"
            }
        },
        {
            "name": "Makefile :: Controlers",
            "scope": [
                "keyword.control.include.makefile"
            ],
            "settings": {
                "foreground": "#CA9286"
            }
        },
        {
            "name": "Makefile :: Variables Declaration",
            "scope": [
                "variable.other.makefile"
            ],
            "settings": {
                "foreground": "#B37468"
            }
        },
        {
            "name": "Makefile :: Variables Use",
            "scope": [
                "string.interpolated.makefile variable.other.makefile"
            ],
            "settings": {
                "foreground": "#ca9286"
            }
        },
        {
            "name": "Makefile :: Variables Ponctuaction",
            "scope": [
                "punctuation.definition.variable.makefile"
            ],
            "settings": {
                "foreground": "#666666"
            }
        },
        {
            "name": "Makefile :: Function Names",
            "scope": [
                "entity.name.function.target.makefile"
            ],
            "settings": {
                "foreground": "#ca7339"
            }
        },
        {
            "name": "Makefile :: Function Prerequisites name",
            "scope": [
                "meta.scope.prerequisites.makefile"
            ],
            "settings": {
                "foreground": "#F0C2A2"
            }
        },
        {
            "name": "Makefile :: Function Escape Continuation Character",
            "scope": [
                "constant.character.escape.continuation.makefile"
            ],
            "settings": {
                "foreground": "#999999"
            }
        },
        {
            "name": "Makefile :: Function Escape Continuation Character teste",
            "scope": [
                "meta.scope.recipe.makefile"
            ],
            "settings": {
                "foreground": "#A9A9A9"
            }
        }
    ]
}
