{
    "name": "Aeon Digital Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#111111",
        "editor.lineHighlightBackground": "#222222",
        "editor.selectionBackground": "#333333",
        "editor.foreground": "#CCCCCC",
        "editorCursor.foreground": "#AAAAAA",
        "editorWhitespace.foreground": "#888888",
        "editorIndentGuide.background": "#333333",
        "editorIndentGuide.activeBackground": "#333333"
    },
    "tokenColors": [
        {
            "name": "Go :: General Code",
            "scope": [
                "source.go"
            ],
            "settings": {
                "foreground": "#EEEEEE"
            }
        },
        {
            "name": "Go :: Comment",
            "scope": [
                "punctuation.definition.comment.go",
                "comment.line.double-slash.go",
                "comment.block.go"
            ],
            "settings": {
                "foreground": "#555555"
            }
        },
        {
            "name": "Go :: Quote",
            "scope": [
                "punctuation.definition.string.begin.go",
                "punctuation.definition.string.end.go"
            ],
            "settings": {
                "foreground": "#BBBBBB"
            }
        },
        {
            "name": "Go :: Quoted Strings : Single; Double; Raw",
            "scope": [
                "string.quoted.single.go",
                "string.quoted.double.go",
                "string.quoted.raw.go"
            ],
            "settings": {
                "foreground": "#DBCF64"
            }
        },
        {
            "name": "Go :: Numeric : Decimal; Hex; Octal",
            "scope": [
                "constant.numeric.decimal.go",
                "constant.numeric.hexadecimal.go",
                "keyword.other.unit.hexadecimal.go"
            ],
            "settings": {
                "foreground": "#9073CE"
            }
        },
        {
            "name": "Go :: Punctuaction : Brackets : Round ()",
            "scope": [
                "punctuation.definition.begin.bracket.round.go",
                "punctuation.definition.end.bracket.round.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Punctuaction : Brackets : Square []",
            "scope": [
                "punctuation.definition.begin.bracket.square.go",
                "punctuation.definition.end.bracket.square.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Punctuaction : Brackets : Curly {}",
            "scope": [
                "punctuation.definition.begin.bracket.curly.go",
                "punctuation.definition.end.bracket.curly.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Punctuaction : comma; semicolon; colon; period",
            "scope": [
                "punctuation.terminator.go",
                "punctuation.other.comma.go",
                "punctuation.other.period.go",
                "punctuation.other.colon.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Punctuaction : address pointer",
            "scope": [
                "keyword.operator.address.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Operators : Assignment( := ); String Concat( + ); Increment( ++ ); Decrement( -- ); Aritmetic",
            "scope": [
                "keyword.operator.assignment.go",
                "keyword.operator.increment.go",
                "keyword.operator.decrement.go",
                "keyword.operator.arithmetic.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Operators : Logical( && || ! ); Comparison( < > <= >= == ); Channel( <- )",
            "scope": [
                "keyword.operator.logical.go",
                "keyword.operator.comparison.go",
                "keyword.operator.channel.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Keywords : Package; Const; Type; Interface; Struct; Func",
            "scope": [
                "keyword.package.go",
                "keyword.const.go",
                "keyword.type.go",
                "keyword.interface.go",
                "keyword.struct.go",
                "keyword.function.go"
            ],
            "settings": {
                "foreground": "#BF598D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Package : Name",
            "scope": [
                "entity.name.type.package.go"
            ],
            "settings": {
                "foreground": "#6586A8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Const : Name",
            "scope": [
                "variable.other.constant.go"
            ],
            "settings": {
                "foreground": "#8B72B0"
            }
        },
        {
            "name": "Go :: Block[Type] : Struct | Interface : Name",
            "scope": [
                "entity.name.type.go"
            ],
            "settings": {
                "foreground": "#8B72B0"
            }
        },
        {
            "name": "Go :: Block[Type] : Struct : Property Name",
            "scope": [
                "variable.other.property.go"
            ],
            "settings": {
                "foreground": "#AAAAAA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Block[Type] : Struct : Type Name || Function : Type Name || Type Name",
            "scope": [
                "storage.type.boolean.go",
                "storage.type.string.go",
                "storage.type.numeric.go",
                "storage.type.rune.go",
                "storage.type.byte.go",
                "storage.type.error.go",
                "entity.name.type.any.go"
            ],
            "settings": {
                "foreground": "#7B8ABA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Block[Type] : Interface : Function : Definition Name || Function : Call",
            "scope": [
                "entity.name.function.support.go"
            ],
            "settings": {
                "foreground": "#A39477"
            }
        },
        {
            "name": "Go :: Block[Type] | Interface : Function : Parameter Name || Block[Func] | : Parameter Name",
            "scope": [
                "variable.parameter.go"
            ],
            "settings": {
                "foreground": "#BBBBBB"
            }
        },
        {
            "name": "Go :: Block[Func] : Definition Name",
            "scope": [
                "entity.name.function.go"
            ],
            "settings": {
                "foreground": "#C49A41"
            }
        },
        {
            "name": "Go :: Keyword : Import",
            "scope": [
                "keyword.control.import.go"
            ],
            "settings": {
                "foreground": "#BF598D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Block[Import] : Brackets",
            "scope": [
                "punctuation.definition.imports.begin.bracket.round.go",
                "punctuation.definition.imports.end.bracket.round.go"
            ],
            "settings": {
                "foreground": "#999999",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Block[Import] : Paths",
            "scope": [
                "entity.name.import.go"
            ],
            "settings": {
                "foreground": "#DBCF64"
            }
        },
        {
            "name": "Go :: Block[Import] : Alias",
            "scope": [
                "variable.other.import.go"
            ],
            "settings": {
                "foreground": "#7B8ABA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Controls : if-else; switch; case; continue; break; defer; go; goto; return",
            "scope": [
                "keyword.control.go",
                "keyword.channel.go"
            ],
            "settings": {
                "foreground": "#787BB4",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Keyword : map",
            "scope": [
                "keyword.map.go"
            ],
            "settings": {
                "foreground": "#52659D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Keyword : var",
            "scope": [
                "keyword.var.go"
            ],
            "settings": {
                "foreground": "#52659D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Keyword : true|false",
            "scope": [
                "constant.language.boolean.go"
            ],
            "settings": {
                "foreground": "#DBBC65",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Keyword : nil",
            "scope": [
                "constant.language.null.go"
            ],
            "settings": {
                "foreground": "#DBBC65",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Builtin",
            "scope": [
                "entity.name.function.support.builtin.go"
            ],
            "settings": {
                "foreground": "#B18F4D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Label",
            "scope": [
                "variable.other.label.go"
            ],
            "settings": {
                "foreground": "#2C7681",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Go :: Variable : Name",
            "scope": [
                "variable.other.assignment.go",
                "variable.other.go"
            ],
            "settings": {
                "foreground": "#BBBBBB"
            }
        }
    ]
}